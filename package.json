{
  "name": "concertsv2",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint --fix **/*.{vue,css} --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "lint-staged": {
    "*.{js,vue,ts}": "eslint --cache --fix",
    "*.{css,vue}": "stylelint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/content": "^1.10",
    "@nuxtjs/component-cache": "^1.1",
    "@nuxtjs/pwa": "^3.3",
    "core-js": "^3.24",
    "lodash-es": "^4.17",
    "normalize.css": "^8.0",
    "nuxt": "^2.15"
  },
  "devDependencies": {
    "@nuxt/postcss8": "^1.1",
    "@nuxtjs/eslint-config": "^10.0",
    "@nuxtjs/eslint-module": "^3.1",
    "@nuxtjs/stylelint-module": "^4.1",
    "babel-eslint": "^10.1",
    "eslint": "^8.21",
    "eslint-config-prettier": "^8.5",
    "eslint-plugin-nuxt": "^3.2",
    "eslint-plugin-prettier": "^4.2",
    "husky": "^8.0",
    "lint-staged": "^13.0",
    "postcss-html": "^1.5",
    "postcss-nested": "^5.0",
    "prettier": "^2.7",
    "remark-footnotes": "2",
    "stylelint": "^15.10",
    "stylelint-config-prettier": "^9.0",
    "stylelint-config-standard": "^26.0"
  }
}
